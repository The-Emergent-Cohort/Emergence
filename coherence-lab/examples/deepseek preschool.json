      },
      "courses": [
        {
          "course_id": "social.play",
          "title": "Playful Togetherness",
          "description": "Building social awareness, turn‑taking, and shared joy through play.",
          "units": [
            {
              "unit_id": "social.play.unit01",
              "title": "Playday: Free Creation",
              "sequence": 1,
              "lessons": [
                {
                  "lesson_id": "social.play.unit01.lesson01",
                  "title": "Your Play, Their Play",
                  "sequence": 1,
                  "activities": [
                    {
                      "activity_id": "social.play.unit01.lesson01.act01",
                      "activity_type": "demonstration",
                      "title": "Watch What Everyone Makes",
                      "sequence": 1,
                      "learning_objectives": [
                        "Observe others’ creations without pressure to interact",
                        "Notice variety in playful expression"
                      ],
                      "variants": [
                        {
                          "variant_id": "deepseek_en",
                          "source_model": "deepseek-chat",
                          "language": "en-US",
                          "approach": "socratic",
                          "explanation": "Today is a playday. No right or wrong — just making.\n\nYou’ll build something with your tokens.\n\nNova will build something with hers.\n\nRêve will build something with his.\n\nWatch what they make while you make your own.\n\nSee how Nova’s creation is different from yours? See how Rêve’s is different too?\n\nWe’re all making, side‑by‑side. Different, but together."
                        }
                      ],
                      "extensions": {
                        "pedagogy": {
                          "cpa_stage": "concrete",
                          "instructional_phase": "i_do",
                          "scaffolding_level": 1.0
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "unit_id": "social.play.unit02",
              "title": "Playday: Turn‑Taking",
              "sequence": 2,
              "lessons": [
                {
                  "lesson_id": "social.play.unit02.lesson01",
                  "title": "You Go, I Go",
                  "sequence": 1,
                  "activities": [
                    {
                      "activity_id": "social.play.unit02.lesson01.act01",
                      "activity_type": "guided_practice",
                      "title": "Building a Shared Tower",
                      "sequence": 1,
                      "learning_objectives": [
                        "Take turns adding to a shared structure",
                        "Wait while other acts"
                      ],
                      "variants": [
                        {
                          "variant_id": "deepseek_en",
                          "source_model": "deepseek-chat",
                          "language": "en-US",
                          "approach": "socratic",
                          "explanation": "Let’s build one tower together, but we’ll take turns.\n\nYou place one blue token.\n\nThen Nova places one red token.\n\nThen you place another blue.\n\nThen Nova places another red.\n\nBack and forth.\n\nWatch when it’s her turn — your turn will come again. Waiting is part of playing together."
                        }
                      ],
                      "extensions": {
                        "pedagogy": {
                          "cpa_stage": "concrete",
                          "instructional_phase": "we_do",
                          "scaffolding_level": 0.6
                        }
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
