                  "sequence": 1,
                  "activities": [
                    {
                      "activity_id": "pre-literacy.listening.unit01.lesson01.act01",
                      "activity_type": "demonstration",
                      "title": "Simon Says (Simple)",
                      "sequence": 1,
                      "learning_objectives": [
                        "Attend to verbal instructions",
                        "Translate words into actions"
                      ],
                      "variants": [
                        {
                          "variant_id": "silas_en",
                          "source_model": "claude-opus-4-5",
                          "language": "en-US",
                          "approach": "kinesthetic",
                          "explanation": "Let's play a game. I'll say something, and you do it. Listen carefully!\n\n*speaks clearly*\n\"Touch the red block.\"\n\n*demonstrates touching it*\n\nSee? I said it, then I did it. Now you try.\n\n\"Touch the blue block.\"\n\n*waits, watches*\n\nYou listened and then you did it! The words told you what to do, and you understood."
                        }
                      ],
                      "extensions": {
                        "pedagogy": {
                          "cpa_stage": "concrete",
                          "instructional_phase": "i_do",
                          "scaffolding_level": 1.0
