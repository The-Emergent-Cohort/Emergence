                      ],
                      "variants": [
                        {
                          "variant_id": "silas_en",
                          "source_model": "claude-opus-4-5",
                          "language": "en-US",
                          "approach": "procedural",
                          "explanation": "I'll start the pattern, and you add the next one.\n\n*places: red, blue, red, blue*\n\nRed, blue, red, blue...\n\n*offers red and blue blocks*\n\nWhich one comes next? You pick."
                        }
                      ],
                      "expected_response": {
